
<div class="modal-header" id="EscogerEmpresaModal">
    <button type="button" class="close" ng-click="vm.cancel()" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title">
        <span>Escoger Agencia</span>
    </h4>
</div>
<div class="modal-body">
    <div class="row clearfix" style="margin-top:10px;">
        <div class="col-sm-12">
            <div class="form-group form-float">
                <div class="form-line">
                    <label for="agenciaSelector"> Seleccione una Agencia: </label><br>
                    <select style="width: 70%;" id="agenciaSelector" ng-options="ag.id as ag.codigo for ag in vm.agencias" ng-model="vm.AgenciaId"></select>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default waves-effect" ng-click="vm.cancel()">
        Cancelar
    </button>
    <button type="button" class="btn btn-primary waves-effect" ng-click="vm.save()">
        Escoger
    </button>
</div>
